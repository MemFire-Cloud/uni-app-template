{
	"easycom": {
	        "autoscan": true,
	        "custom": {
	            // uni-ui 规则如下配置
	            "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
	        }
	    },
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/tabBar/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				"app-plus": {
					"bounce": "vertical",
					"titleNView": {
						"buttons": [{
							"text": "\ue534",
							"fontSrc": "/static/uni.ttf",
							"fontSize": "22px",
							"color": "#FFFFFF"
						}]
					}
				}
			}
		},
		{
			"path": "pages/tabBar/me/me",
			"style": {
				"navigationBarTitleText": "我的",
				"app-plus": {
					"bounce": "vertical",
					"titleNView": {
						"buttons": [{
							"text": "\ue534",
							"fontSrc": "/static/uni.ttf",
							"fontSize": "22px",
							"color": "#FFFFFF"
						}]
					}
				}
			}
		},
		{
			"path": "pages/login/index",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "pages/phone-login/index",
			"style": {
				"navigationBarTitleText": "手机认证"
			}
		},
		{
			"path": "pages/messages/index",
			"style": {
				"navigationBarTitleText": "聊天室"
			}
		},
		{
			"path": "pages/userinfo/index",
			"style": {
				"navigationBarTitleText": "用户信息"
			}
		},
		{
			"path": "pages/todo/index",
			"style": {
				"navigationBarTitleText": "todoList"
			}
		},{
			"path": "pages/filestorage/index",
			"style": {
				"navigationBarTitleText": "网盘"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#007AFF",
		"borderStyle": "black",
		"backgroundColor": "#F8F8F8",
		"list": [{
				"pagePath": "pages/tabBar/index/index",
				"iconPath": "static/index_off.png",
				"selectedIconPath": "static/index_on.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/tabBar/me/me",
				"iconPath": "static/mine_off.png",
				"selectedIconPath": "static/mine_on.png",
				"text": "我的"
			}
		]
	}
}
